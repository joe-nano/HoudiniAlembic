node = kwargs['node']
oldversion = kwargs['old_version']

if oldversion == '':
    parm = node.parm('fileName')
    if not parm.isAtDefault(False, True):
        node.parm('numFiles').set(1)
        node.parm('enableFile1').set(1)
        node.parm('fileName1').setFromParm(parm)
        parm.set('')
